<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.rexben.associateexam1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".Calculator" />
        <activity android:name=".ColorActivity" />
        <activity
            android:name=".ImageActivity"
            android:label="@string/title_activity_image"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OrderActivity"
            android:label="Order Activity"
            android:parentActivityName=".ImageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ImageActivity" />
        </activity>
        <activity android:name=".DialNumbers" />
        <activity android:name=".ToastActivity" />
        <activity android:name=".PickerActivity" />
        <activity
            android:name=".TabbingActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NavBar"
            android:label="@string/title_activity_nav_bar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RecyclerActivity"
            android:label="@string/title_activity_recycler"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ScoreKeeper" />
        <activity android:name=".BatteryCharging" />
        <activity android:name=".MaterialMe"></activity>
    </application>

</manifest>